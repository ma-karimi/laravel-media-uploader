{
    "name": "ma-karimi/laravel-media-uploader",
    "description": "This package used to upload files using laravel-media-library | Forked from ahmed-aliraqi/laravel-media-uploader",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Amin Karimi",
            "email": "karimi.mak@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.3",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "spatie/laravel-medialibrary": "^9.0|^10.0|^11.0",
        "php-ffmpeg/php-ffmpeg": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0|~5.3|~6.0|~7.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "AhmedAliraqi\\LaravelMediaUploader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AhmedAliraqi\\LaravelMediaUploader\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AhmedAliraqi\\LaravelMediaUploader\\Providers\\UploaderServiceProvider"
            ]
        }
    }
}
